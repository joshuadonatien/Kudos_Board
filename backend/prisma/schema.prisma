generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Board {
    board_id        Int     @id @default(autoincrement())
    title           String
    image_url       String
    category        String
    author          String?
}

model Card {
    card_id         Int     @id @default(autoincrement())
    title           String
    description     String
    gif_url         String
    upvotes         Int
    owner           String?
}